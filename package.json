{
  "name": "pilou",
  "version": "0.0.1",
  "description": "http crud library for making web requests with promises",
  "main": "pilou.js",
  "scripts": {
    "test": "ava test --require babel-core/register && xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumevincent/pilou.git"
  },
  "keywords": [
    "http",
    "crud",
    "promise"
  ],
  "author": "Guillaume Vincent <guillaume@oslab.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guillaumevincent/pilou/issues"
  },
  "homepage": "https://github.com/guillaumevincent/pilou#readme",
  "dependencies": {
    "axios": "^0.13.1",
    "es6-template-strings": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "nock": "^8.0.0",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "envs": [
      "browser"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "comments": false
  }
}
